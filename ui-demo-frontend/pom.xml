        </dependency>
        <dependency>
            <groupId>org.dominokit.domino</groupId>
            <artifactId>spin</artifactId>
            <version>${project.version}</version>
            <classifier>sources</classifier>
        </dependency>
        <dependency>
            <groupId>org.dominokit.domino</groupId>
            <version>${project.version}</version>
            <artifactId>inputfields</artifactId>
        </dependency>
        <dependency>
            <groupId>org.dominokit.domino</groupId>
            <artifactId>inputfields</artifactId>
            <version>1.0-SNAPSHOT</version>
            <classifier>sources</classifier>
        </dependency>
        <dependency>
